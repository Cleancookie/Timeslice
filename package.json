{
  "name": "Timeslice",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Alex Law final year project - a project management solution",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "serve": "npm run serve:dev",
    "serve:dev": "adonis serve --dev --debug",
    "serve:prod": "adonis serve",
    "build": "npm run build:dev",
    "build:dev": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build:prod": "npm run build:production",
    "build:production": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@babel/polyfill": "^7.2.5",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "jquery-ui": "^1.12.1",
    "lodash": "^4.17.11",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.24.0",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "adonisjs-types": "^1.3.4",
    "bootstrap": "^4.1",
    "cross-env": "^5.2.0",
    "jquery": "^3.3.1",
    "laravel-mix": "^4.0.13",
    "popper.js": "^1.14.6",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "autoload": {
    "App": "./app"
  }
}
